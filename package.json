{
  "name": "nfc",
  "version": "1.0.0",
  "description": "Web-NFC api in form of a React hook",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "rollup -c -w",
    "build": "npm run rollup-step && npm run tsc-step && npm run postbuild-step",
    "rollup-step": "rollup -c",
    "tsc-step": "tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "postbuild-step": "node ./postbuild.js",
    "publish": "npm run build && npm publish dist"
  },
  "keywords": [],
  "author": "inhopj",
  "license": "MIT",
  "repository": "inhopj/nfc",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.75.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
